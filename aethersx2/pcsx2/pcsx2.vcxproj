      <PreprocessorDefinitions Condition="!$(Configuration.Contains(AVX2)) And '$(Platform)'!='ARM64'">_M_SSE=0x401;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <PreprocessorDefinitions Condition="$(Configuration.Contains(AVX2)) And '$(Platform)'!='ARM64'">_M_SSE=0x501;%(PreprocessorDefinitions)</PreprocessorDefinitions>
